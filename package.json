{
  "name": "connect4",
  "version": "1.0.0",
  "author": "Sasha Goldenson <sgoldens@gmail.com> https://github.com/sgoldens",
  "description": "Connect Four",
  "license": "MIT",
  "main": "connect4.js",
  "scripts": {
    "build": "./node_modules/.bin/browserify -r underscore -r store -r bootstrap -r ./src/Player.js:player -r ./src/Board.js:board  > public/bundle.js",
    "test": "./node_modules/karma/bin/karma start karma.conf.js --single-run --",
    "test:tdd": "karma start --auto-watch --no-single-run --",
    "codecov": "cat ./coverage/*/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "karma-growl-reporter": "^1.0.0",
    "npm": "^5.7.1",
    "store": "^2.0.12",
    "underscore": "^1.8.3",
    "watchify": "^3.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^16.1.1",
    "browserify-istanbul": "^2.0.0",
    "chai": "^4.1.2",
    "codecov.io": "^0.1.6",
    "istanbul": "^0.4.4",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "karma": "^2.0.0",
    "karma-browserify": "^5.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.2",
    "mocha": "^5.0.4",
    "sinon": "^4.4.6",
    "sinon-chai": "^2.8.0"
  }
}
